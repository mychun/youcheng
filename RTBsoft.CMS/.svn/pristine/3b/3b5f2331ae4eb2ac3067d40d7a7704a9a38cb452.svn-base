/* global */
* {word-break:break-all;font-family:"Microsoft Yahei",Verdana,Arial;-webkit-text-size-adjust:none; outline: none;}/*"Microsoft Yahei",*/
body {margin:0;font-size:12px;color:#333;background:#fff;line-height: 1.42857143;}
form,input,select,textarea,td,th {font-size:12px;}
table{width: 100%;border-spacing: 0px;border-collapse: collapse;}
img {border:none;}
ul li {list-style-type:none;}
ol li {list-style-type:decimal;}
ul,form,input {margin:0px;padding:0px;}
dl,dd,h1,h2,h3,h4,p{ margin: 0;}
a:link,a:visited,a:active {color:#333;text-decoration:none;}
a:hover {color:#0086da;}
.section {margin:auto;width:980px;}
.fl {float:left;}
.fr {float:right;}
i{ font-style: normal;}
.clear{ clear:both;}
.clearfix:after { content:"."; display:block; height:0; clear:both; visibility:hidden; } 
.txt-center{ text-align: center;}
.txt-right{ text-align: right;}
.bg-white{ background-color: #fff;}
.bg-gray{ background-color:#fafafa;}
.display-none{ display:none;}
.f-gay{ color:#717171;}

.mt4{ margin-top:4px;}
.mt10{ margin-top:10px;}
.mt30{ margin-top:30px;}
.ml40{ margin-left:40px;}
.pt30{ padding-top:30px;}
.pt40{ padding-top:40px;}
.pt80{ padding-top: 80px;}
.ptd50{ padding: 50px 0;}
.h40{ height:40px;}
.fs16{ font-size:16px;}

.spaceTd{ padding: 50px 0 70px;}

/*头部*/
.header{ position:absolute; left:0; top:0; width:100%; z-index:10;}
.logo-wrap{ background-color:rgba(20,20,21, 0.5); padding: 20px 20px;}
.logo-wrap img{ display:block; width: 63px; height:39px; margin: 0 auto;}
.logo-ch{display:block;text-align:center;color: #afafaf;font-size: 20px;margin-top: 10px;}
.logo-en{ display:block; text-align:center; margin-top:3px; color:#999;}

.nav-box{ margin-top: 48.5px;}
.nav-box > a{ float:left; margin-left:40px; padding: 8px 0px; border-bottom: 2px solid transparent; font-size:16px; color:#fff; transition:all 0.5s; -webkit-transition:all 0.5s;}
.nav-box > a:first-child{ margin-left:0;}
.nav-box > a:hover{ color:#fff; border-bottom-color:#fff;}

#index .p-index,
#productService .p-productService,
#news .p-news,
#helpCenter .p-helpCenter,
#about .p-about{ color:#fff; border-bottom-color:#fff;}

/*尾部*/
.footer{ background: url(../images/world.jpg); padding: 50px 0 0;}
.wrap-cont{ float:left; margin-left: 60px; width: 200px;}
.wrap-cont:first-child{ margin-left: 0;}
.wrap-cont dt{ padding-bottom:10px; font-size:18px; color:#fff;}
.wrap-cont dd p{ margin-top:8px; color:#EEE;}
.wrap-cont dd p .link{ color:#EEE; font-size: 13px;}
.link:hover{ text-decoration:underline;}

.foot-logo{ float:right; width: 200px;}
.foot-logo img{ display:block; width: 63px; height:39px; margin: 0 auto;}
.foot-logo .logo-ch{ color: #fff;}
.foot-logo .logo-en{ color:#999;}

.bottom-t{ padding-top:30px; margin-top:30px; padding-bottom:30px; border-top:1px solid #929292; text-align:center; color:#fff;}
.bottom-t a{ color:#fff;}

/*登录*/
.login-wrap{ width:290px; position:absolute; left: 50%; top:50%; margin-left:-145px; margin-top:-180px; background-color:#fff; border:1px solid #f1f1f1; border-radius:4px;}
.login-logo{ padding: 25px 0 10px; text-align:center;}
.login-f{ padding:15px 15px 40px;}
.l-f-row{ margin-top:15px;}
.l-f-row:first-child{ margin-top:0;}
.l-f-control{display: block; width: 234px; padding: 6px 12px;  font-size: 14px; border: 1px solid #e1e1e1;  border-radius: 4px; transition:all .3s; -webkit-transition:all .3s;}
.l-f-control:focus{ border-color: #2c558a; outline: 0; -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(0,134,218,.6); box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(0,134,218,.6);}
.check-l{ float:left; cursor:pointer;}
.check-l input{ display:inline-block; margin-right:3px; position:relative; top:2px;}
.l-f-bottom{ margin-top:25px;}
.submit-btn,
.get-code{ display:block; width:100%; border-radius:4px; line-height:45px; height:45px; color:#fff; font-size:16px; text-align:center; cursor:pointer; background-color:#0086da; border:none; transition: background-color .3s; -webkit-transition: background-color .3s;}
.submit-btn:hover,
.get-code:hover{background-color:#2c558a;}

.hint-info{ display:none; position:absolute; left:15px; bottom:230px; color:#d9534f;}

/*注册*/
.register-wrap{ margin:40px auto; width:980px; background-color:#fff; padding: 80px 0;}
.reg-title{ padding-bottom:10px; font-size:22px; text-align:center; font-weight:bold; position:relative;}
.return-login{ font-weight:normal; font-size:14px; position:absolute; right:20px; top:5.5px;}
.reg-from{ width:450px; margin: 0 auto;}
.reg-from .l-f-row{ padding-left:100px; position:relative; margin-top:30px;}
.must-h{ color:#d81e06; font-size:16px; margin-right:2px;}
.label-name{ position:absolute; left:0px; text-align:right; width:95px; font-size:14px; line-height:33px; font-weight:bold;}
.code-row.l-f-row .l-f-control{ width: 134px;}
.get-code{ position: absolute; top:0; left: 270px; text-align:center; line-height:33px; height:33px; width: 90px; font-size:12px;}
.get-code.done{ background-color:#e1e1e1; cursor:not-allowed;}

.error-info-txt{ position:absolute; left: 370px; white-space:nowrap; top: 0px; line-height:33px; color:#d81e06; padding-left:18px; background: url(../images/gantanhao.png) no-repeat; background-position: center left; font-size:14px;}

.upload{ display:inline-block; width: 100px; margin-right:27px;}
.upload-img,
.b-txt{ display:inline-block; width: 100%;}
.upload-img{ border: 1px solid #e1e1e1; border-radius: 4px; overflow:hidden;}
.upload-img img{ display:inline-block; width:100%;}
.b-txt{ margin-top: 5px; text-align:center; font-size:14px; color:#717171;}

.reg-from .l-f-row.custom-check{ margin-top:16px;}
.custom-check .check-l{ font-size:14px; cursor:default;}
.custom-check a{ float:left; margin-left:3px; font-size:14px;}
.reg-from .submit-btn{ width:260px;}

/*首页*/
.swiper-slide img{ display:block; width:100%;}
.swiper-button-prev, .swiper-button-next{ margin-top:-24px; width:27px; height:48px; color:#fff; background-color:rgba(0,0,0,.5); background-size: 14px auto;}
.swiper-button-prev{ background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%20viewBox%3D'0%200%2027%2044'%3E%3Cpath%20d%3D'M0%2C22L22%2C0l2.1%2C2.1L4.2%2C22l19.9%2C19.9L22%2C44L0%2C22L0%2C22L0%2C22z'%20fill%3D'%23ffffff'%2F%3E%3C%2Fsvg%3E");}
.swiper-button-next{ background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg%20xmlns%3D'http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg'%20viewBox%3D'0%200%2027%2044'%3E%3Cpath%20d%3D'M27%2C22L27%2C22L5%2C44l-2.1-2.1L22.8%2C22L2.9%2C2.1L5%2C0L27%2C22L27%2C22z'%20fill%3D'%23ffffff'%2F%3E%3C%2Fsvg%3E");}
.swiper-button-prev:hover,
.swiper-button-next:hover { background-color:rgba(0,0,0,.8);}
.swiper-pagination{ position: absolute; bottom: 10px;  width:100%; height:10px; bottom:15px; line-height:10px;} 
.swiper-pagination .swiper-pagination-switch{display:inline-block; margin:0px 5px !important;width:10px;height:10px; background-color:#666; border-radius:50%; cursor:pointer;} 
.swiper-pagination .swiper-active-switch{background:#000;}

.fixed-txt{ position:absolute; left:0; top:50%; width:100%; margin-top: -59.5px; text-align:center; color:#fff;}
.fixed-txt dt{ font-size:60px; font-weight:700;}
.fixed-txt dd{ font-size:24px;}
.about-wrap{background: url("../images/world.jpg"); background-repeat: no-repeat; background-size: cover;}

.title{text-align: center; color: #FFF; font-size: 35px; margin-bottom: 30px;}

.list-dl{ width:300px; margin-left:20px; float:left; background-color:#fff;}
.list-dl:first-child{ margin-left:0;}
.list-dl dt{ overflow:hidden;}
.list-dl dt img{ display:block; width:100%; transition: all 2s ease-in-out 0s; -webkit-transition: all 2s ease-in-out 0s; -moz-transition: all 2s ease-in-out 0s; -o-transition: all 2s ease-in-out 0s;}
.list-dl dt img:hover {transform: scale(1.3); -webkit-transform: scale(1.3);  -moz-transform: scale(1.3); -ms-transform: scale(1.3);}
.list-dl dd{ padding:0 20px;}
.list-dl dd h3{padding-top: 15px;}
.list-dl dd h3 span{display:inline-block;padding-bottom: 15px;color: #009688;border-bottom: 3px solid #395B74;font-size: 20px;font-weight: 700;}
.list-dl dd p{padding: 13px 0 20px;font-size: 14px;font-weight: 500;line-height: 25px;color: #9C9999;}

.title.block-txt{ color:#333;}

.subhead{ text-align: center;}
.subhead span{ display:inline-block; color:#5f7d8b; padding: 0 10px; padding-bottom: 25px; font-size: 20px;margin: 0 auto; border-bottom: 3px solid #395B74;}

.info-list{ margin-top: 50px;}
.info-dl{float:left;position:relative;padding-left: 96px;margin-top:40px;}
.info-dl dt{ position: absolute; left: 0; top: 0px; height: 74px; width: 80px;}
.info-dl dt:before{ content: ''; display: inline-block; background: url(../images/img.png) no-repeat; vertical-align: top;}
.info-dl .icon2:before{height:74px;width:75px;background-position:0 0;}
.info-dl .icon1:before{height:76px;width:76px;background-position:-75px 0;}
.info-dl .icon4:before{height:65px;width:80px;background-position:-151px 0;}
.info-dl .icon3:before{height:69px;width:80px;background-position:-231px 0;}
.info-dl dd{width: 374px;}
.info-dl dd h3{font-size: 25px;font-weight: 700;color: #009688;padding-bottom: 8px;}
.info-dl dd p{ font-size: 14px; line-height: 30px; color: #A09393;}

.l-fl{width: 421px;}
.l-fl img{ display:block; width:100%;}
.l-fr{width: 510px;}

.new-ul li{ height:46px; line-height:46px; border-top: 1px #d7d7d7 dashed; background:url(../images/clip_10.png) no-repeat left center; padding-left:10px}
.new-ul li:first-child{ border-top:none;}
.new-ul .new-title{ display:inline-block; font-size:14px; max-width:390px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; color:#595959; transition: all .3s; -webkit-transition: all .3s;}
.new-ul .new-title:hover{ color:#3399FF;}
.new-time{ color:#a7a7a7; font-size:14px;}

/*产品服务*/
.custom-title.title{ font-size: 27px; color:#333;}
.top-wrap{ background-image: url(../images/banner2.png); background-repeat: no-repeat;  background-position: 50% 50%; background-size: cover; position:relative; height: 416px;}
.title-lump{ color:#fff; font-size:36px; position:absolute; left:0; bottom: 120px; width: 100%; text-align:center;}
.custom-list .list-dl{ background-color:#fafafa;}

/*新闻资讯*/
.list-row{ padding: 40px 0px 40px 255px; height: 155px; position:relative; border-top: 1px solid #e1e1e1;}
.list-row:first-child{ border-top:none;}
.news-img{ position:absolute; left:0; top: 40px; width:240px; height: 155px;}
.news-img img{ display:block; width:100%; height: 100%;}
.news-title{ font-size: 18px; font-weight:normal;}
.news-info{ color:#9C9999; margin-top: 15px; font-size: 14px; max-height: 60px; overflow:hidden;}
.news-time{ color: #888; margin-top: 5px; font-size: 14px;}

/*详情*/
.details h3{ font-size:26px; margin-bottom:35px; text-align:center;}
.details p{ text-indent:2em; line-height:25px; font-size:14px; color:#595959;}

/*帮助中心*/
.fl-nav{ float:left; width:200px;}
.fl-n-row{ display:block; margin-bottom: 10px; font-size:14px; padding-left:17px; position:relative; cursor:pointer;}
.fl-n-row::before{ content:''; position:absolute; left:0; top:50%; width:16px; height:16px; margin-top:-8px; background:url(../images/right.png) no-repeat; background-size: 100% 100%;}
.fl-n-row.sel{ color:#0086da; cursor:default;}
.fr-cont{ float:right; width:740px;}
.list-r-title{ font-size:27px; margin-bottom:30px; text-align:center;}
.static-info{ font-size:14px; line-height:25px;}


/*关于我们*/
.l-fl{width: 421px;}
.l-fl img{ display:block; width:100%;}
.l-fr{width: 510px;}
.row-p{font-size: 15px;text-indent:2em;line-height:26px;margin-top: 27px;color:#737373;}
.row-p:first-child{ margin-top:0px;}

.info-lump{ margin-top: 20px; border-top: 1px solid #e1e1e1; padding-top: 20px;}
.title-h{ font-size: 27px;}
.txt-p{ font-size: 14px; margin-top: 10px; color:#737373;}