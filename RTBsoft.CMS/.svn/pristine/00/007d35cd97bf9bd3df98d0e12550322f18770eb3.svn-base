.search_bt .sstext:focus{ border-color: #0086da; outline: 0; -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 3px rgba(0,134,218,.6); box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 3px rgba(0,134,218,.6);}
