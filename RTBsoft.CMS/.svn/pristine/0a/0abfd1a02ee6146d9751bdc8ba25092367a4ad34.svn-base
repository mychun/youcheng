/* global */
* {word-break:break-all;font-family:"Microsoft Yahei",Verdana,Arial;-webkit-text-size-adjust:none; outline: none;}/*"Microsoft Yahei",*/
body {margin:0;font-size:14px;color:#333;background:#fff;line-height: 1.42857143;}
form,input,select,textarea,td,th {font-size:12px;}
table{width: 100%;border-spacing: 0px;border-collapse: collapse;}
img {border:none;}
ul li {list-style-type:none;}
ol li {list-style-type:decimal;}
ul,form,input {margin:0px;padding:0px;}
dl,dd,h1,h2,h3,h4,p{ margin: 0;}
a:link,a:visited,a:active {color:#333;text-decoration:none;}
a:hover {color:#0659b8;}
.m {margin:auto;width:1000px;}
.fl {float:left;}
.fr {float:right;}
i{ font-style: normal;}
.clear{ clear:both;}
.clearfix:after { content:"."; display:block; height:0; clear:both; visibility:hidden; } 
.txt-center{ text-align: center;}
.txt-right{ text-align: right;}
.bg-white{ background-color: #fff;}
.bg-gray{ background-color:#f8f8f8;}
.display-none{ display:none;}

.mt4{ margin-top:4px;}
.mt30{ margin-top:30px;}

/*头部*/
.header{ background-color:#0659b8; height: 88px;}
.nav{ margin-top:35px; margin-left:100px; position:relative;}
.nav .nav-list{ float:left;  padding:0 20px; font-weight: bold;text-align: center; line-height: 32px; height: 32px; font-size:14px; color:#fff; position:relative; z-index:2;}
.nav .nav-list:hover{ transition: all .3s; -webkit-transition: all .3s; color:#99d4ff;}
.nav .nav-list.active + .active-sel{}

/*.active-sel{position: absolute; left: 0; top: -3px; border: 3px solid #fff;  height: 32px; width: 62px;}
.sel-bg{ margin:0 -3px; border-right:3px solid #0659b8; border-left:3px solid #0659b8; height:18px; top:7px; left:0; right:0; position:absolute;}*/ 
.active-sel{position: absolute; left: 0; bottom: -8px; border-bottom: 3px solid #fff;  height: 32px; width: 60px;}

.head-fr{ margin-top:36px;}
.head-fr .head-user{transition: all .3s; -webkit-transition: all .3s; border:2px solid #fff; width: 48px; line-height:24px; display:inline-block; text-align:center; color:#fff; margin-left:5px;}
.head-fr .head-user:hover{ border-color:#99d4ff;}
.head-fr .head-user:first-child{ margin-left:0;}

/*尾部*/
.footer{ padding:20px 0; border-top:1px solid #e1e1e1; background-color:#f8f8f8;}
.footer .m{ position:relative;}
.link-row a{ display:inline-block; margin-right:15px;}
.area-info{ margin-top:3px; color:#666666;}
.bottom-txt{ padding-top:20px; margin-top:20px; text-align:center;}
.other-txt{ position:absolute; white-space:nowrap; right:0; top:50%; margin-top:-10px;}

/*登录*/
.login-wrap{ width:290px; position:absolute; left: 50%; top:50%; margin-left:-145px; margin-top:-180px; background-color:#fff; border:1px solid #e1e1e1; border-radius:4px;}
.login-logo{ padding: 25px 0 10px; text-align:center;}
.login-f{ padding:15px 15px 40px;}
.l-f-row{ margin-top:15px;}
.l-f-row:first-child{ margin-top:0;}
.l-f-control{display: block; width: 234px; padding: 6px 12px;  font-size: 14px; border: 1px solid #e1e1e1;  border-radius: 4px;}
.check-l{ float:left; cursor:pointer;}
.check-l input{ display:inline-block; margin-right:3px; position:relative; top:2px;}
.l-f-bottom{ margin-top:25px;}
.login-btn{ display:block; width:100%; border-radius:4px; line-height:45px; height:45px; color:#fff; font-size:16px; text-align:center; cursor:pointer; background-color:#0659b8; border:none; transition: all .3s; -webkit-transition: all .3s;}
.login-btn:hover{background-color:#144884;}

/*首页*/
.top-wrap{ position:relative;}

.slides-box { width: 100%; height: 360px; margin: auto; overflow: hidden; position:relative;} 
.slides { position: relative; height: 100%; width: 100%; margin: auto; float: left; } 
.slides li { position: absolute; width:  100%; height: 100%; } 
.slides li img{ display: block; width: 100%; margin: auto; } 
.prev, .next { display:none;} 
.pagination{ position: absolute; bottom: 10px; z-index: 99; width:100%; height:5px; text-align:center;} 
.pagination li{display:inline-block; margin:0px 3px; } 
.pagination li a{ display:inline-block;width:14px;height:14px;width:25px; height:5px; overflow:hidden;vertical-align:middle;background:#e2e7e7;} 
.pagination .active{background:#0659b8;}

.top-fix{ position:absolute; width:1000px; height:360px; left:50%; margin-left:-500px; top:0px;}
.fix-wrap{ width:320px; height:274px; background-color:#fff; margin-top:43px;}
.fix-head{ height:42px; background-color:#0659b8;}
.fix-head span{ float:left; width:50%; line-height:42px; height:42px; text-align:center; font-weight:bold; color:#fff; cursor:pointer;}
.fix-head .active{ background-color:#20599c; position:relative; cursor:default;}
.fix-head .active::after{ content:''; height:2px; width:100%; position:absolute; left:0px; top:0; background-color:#4f98cd;}

.fix-cont{ padding:15px;}
.f-row{ position:relative; padding-left:65px; margin-top:15px;}
.f-row:first-child{ margin-top:0;}
.f-name{ position:absolute; left:0px; top:50%; margin-top:-8px; font-weight:bold; text-align:right; width:60px;}
.f-control,
.f-select{ display:inline-block; width: 170px; border:1px solid #e1e1e1; height:26px; line-height:26px; padding:0 5px; border-radius:3px;}
.f-control:focus,
.f-select:focus,
.l-f-control:focus,
.txt-area:focus{ transition:all .3s; -webkit-transition:all .3s; border-color: #4f98cd; outline: 0; -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6); box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6);}
.f-control:disabled{ background-color:#e1e1e1;}
.f-select{ width:182px; height:28px;}
.area-sel .f-control{ width: 64px; text-align:center;}
.to-next{ line-height:28px; display:inline-block; padding:0 5px;}
.unit-txt{ line-height:28px; display:inline-block; margin-left:5px;}
.volume-sel .f-control{ width:31px;}
.no-pl-row.f-row{ padding-left:0;}
.f-btn{ border-radius:3px; background-color:#4f98cd; height:30px; line-height:30px; text-align:center; display:block; width:188px; color:#fff; margin:0 auto; cursor:pointer; border:none;}
.f-btn:hover{ transition: all .3s; -webkit-transition: all .3s; background-color:#4384b3;}
.txt-area{ display:inline-block; height:143px; width:278px; border:1px solid #e1e1e1; padding:5px; border-radius: 3px;}

.title-wrap{ padding-top:70px; position:relative;}
.title-wrap .line{ width: 100%; height: 1px;  background-color: #ddd;  position: absolute; bottom: 15.5px;left: 0;}
.title-wrap .tit_bt{ width:195px; height:31px; line-height:31px; font-size:22px; background-color:#fff; margin:0 auto; text-align:center; position:relative;z-index:1;}
.bg-gray .title-wrap .tit_bt{ background-color:#F8F8F8;}

.new-wrap{ padding-top:57px; padding-bottom:70px;}

.new-fl{ width:440px;}
.new-fl img{ display:block; width:100%;}
.new-fr{ width:520px;}
.new-ul li{ height:46px; line-height:46px; border-top: 1px #d7d7d7 dashed; background:url(../images/clip_10.png) no-repeat left center; padding-left:10px}
.new-ul li:first-child{ border-top:none;}
.new-ul .new-title{ display:inline-block; max-width:390px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; color:#595959;}
.new-ul .new-title:hover{ transition: all .3s; -webkit-transition: all .3s; color:#0659b8;}
.new-time{ color:#a7a7a7;}

/*列表*/
.list-wrap{ margin-top:30px;}
.list-fl{ width:220px; background-color:#fff; float:left;}
.title{ padding:16px 0; text-align:center; background-color:#0659b8; color:#fff;}
.fl-nav{ padding:20px 0;}
.fl-nav .nav-l{ display:block; padding-left:30px; line-height:40px; height:40px; color:#333; transition:all .2s; -webkit-transition:all .2s;}
.fl-nav .nav-l.active{ background-color:#4c4c4c; cursor:default; color:#fff;}
.fl-nav .nav-l:hover{ background-color:#4c4c4c; color:#fff;}

.list-fr{ float:right; width:710px; background-color:#fff; padding:20px;}
.list-title{ font-size:18px; margin-bottom:10px;}
.list-fr .new-ul .new-title{ max-width:585px;}