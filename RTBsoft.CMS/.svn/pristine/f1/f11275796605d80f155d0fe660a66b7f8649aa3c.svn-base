using System;

namespace RTBcms.Web.Plugin.OnlineService.Model
{
	[Serializable]
	public class online_service
	{
		private int _id;

		private string _service_template = string.Empty;

		private string _service_title = string.Empty;

		private int _sort_id = 99;

		private int _is_lock = 0;

		private DateTime _add_time = DateTime.Now;

		private string _site_path = string.Empty;

		public int id
		{
			get
			{
				return this._id;
			}
			set
			{
				this._id = value;
			}
		}

		public string service_template
		{
			get
			{
				return this._service_template;
			}
			set
			{
				this._service_template = value;
			}
		}

		public string service_title
		{
			get
			{
				return this._service_title;
			}
			set
			{
				this._service_title = value;
			}
		}

		public int sort_id
		{
			get
			{
				return this._sort_id;
			}
			set
			{
				this._sort_id = value;
			}
		}

		public int is_lock
		{
			get
			{
				return this._is_lock;
			}
			set
			{
				this._is_lock = value;
			}
		}

		public DateTime add_time
		{
			get
			{
				return this._add_time;
			}
			set
			{
				this._add_time = value;
			}
		}

		public string site_path
		{
			get
			{
				return this._site_path;
			}
			set
			{
				this._site_path = value;
			}
		}
	}
}
