.next-box{ margin-top: 40px;}
.next-box p{ font-size: 14px;}
.next-box p span{ color:#868686;}

/*关于*/
.about-cont{ padding: 40px; background-color:#fff;}

.about-row{ margin-top:80px;}
.about-row:first-child{ margin-top:0;}

.title-wrap{ position:relative; margin-bottom: 40px;}
.title-wrap .line{ width: 100%; height: 1px;  background-color: #f1f1f1;  position: absolute; bottom: 15.5px;left: 0;}
.title-wrap .tit_bt{ width:195px; height:31px; line-height:31px; font-size:28px; background-color:#fff; margin:0 auto; text-align:center; position:relative;z-index:1;}

.lump-wrap{ padding-top:40px;}
.l-fl{width: 421px;}
.l-fl img{ display:block; width:100%;}
.l-fr{width: 446px;}
.row-p{font-size: 16px;text-indent:2em;line-height:26px;margin-top: 9px;}
