<?xml version="1.0" encoding="utf-8"?>
<urls>
  <rewrite name="index" type="index" page="index.aspx" inherit="RTBcms.Web.UI.Page.index" templet="index.html">
    <item path="index.aspx" pattern="index.aspx" />
  </rewrite>
  <rewrite name="about" type="other" page="about.aspx" inherit="RTBcms.Web.UI.Page.index" templet="about.html" />
  <rewrite name="contact" type="other" page="contact.aspx" inherit="RTBcms.Web.UI.Page.index" templet="contact.html" />
  <rewrite name="faq" type="other" page="faq.aspx" inherit="RTBcms.Web.UI.Page.index" templet="faq.html" />
  <rewrite name="partner" type="other" page="partner.aspx" inherit="RTBcms.Web.UI.Page.index" templet="partner.html" />
  <rewrite name="recruit" type="other" page="recruit.aspx" inherit="RTBcms.Web.UI.Page.index" templet="recruit.html" />
  <rewrite name="feedback" type="plugin" page="feedback.aspx" inherit="RTBcms.Web.Plugin.Feedback.feedback" templet="feedback.html">
    <item path="feedback.aspx" pattern="feedback.aspx" />
    <item path="feedback/{0}.aspx" pattern="feedback/(\w+).aspx" querystring="page=$1" />
  </rewrite>
  <rewrite name="mfeedback" type="plugin" page="mfeedback.aspx" inherit="RTBcms.Web.Plugin.Feedback.feedback" templet="mfeedback.html">
    <item path="mfeedback.aspx" pattern="mfeedback.aspx" />
    <item path="mfeedback/{0}.aspx" pattern="mfeedback/(\w+).aspx" querystring="page=$1" />
  </rewrite>
  <rewrite name="link" type="plugin" page="link.aspx" inherit="RTBcms.Web.Plugin.Link.link" templet="link.html">
    <item path="link.aspx" pattern="link.aspx" />
  </rewrite>
</urls>